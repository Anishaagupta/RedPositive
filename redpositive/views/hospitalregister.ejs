<!-- register form -->
	<div id="colorlib-choose">
		<div class="container-fluid">
			<div class="row">
				<div class="choose">
					<div class="half img-bg" style="background-image: url(/images/latest_2.jpg);"></div>
					<div class="half features-wrap">
						<div class="features-services animate-box">
							<div class="row">
								<h1 style="text-align: center;color: #ff2121;">Sign Up</h1>
								<div style="width: 50%; margin: 25px auto;font-family: 'Montserrat', sans-serif;">
									<form>
										<div class="form-group">
											<label for="etype">Entity Type </label>
											<select name="etype" class="form-control" id="etype" onchange="choice()">
												<option disabled selected value> -- Select an option -- </option>
												<option value="Clinic">Clinic</option>
												<option value="Hospital">Hospital</option>
											</select>
										</div>
										<div class="form-group">
											<label for="ename"><span id="entity1"></span> Name </label>
											<input id="ename" type="text" name="ename" class="form-control" placeholder="Enter name.." required>
										</div>
										<div class="form-group">
											<label for="ereg"><span id="entity2"></span> Registration Number </label>
											<input id="ereg" type="text" name="ereg" class="form-control" placeholder="Enter Registration number.." required>
										</div>
										<div class="form-group">
											<label for="enum"><span id="entity3"></span> Contact Number </label>
											<input id="enum" type="number" maxlength="10" name="enum" class="form-control" placeholder="Enter Contact number.." required>
										</div>
										<div class="form-group">
											<label for="eiso"><span id="entity4"></span> ISO Number </label>
											<input id="eiso" type="text" name="eiso" class="form-control" placeholder="Enter ISO number.." required>
										</div>
										<div class="form-group">
											<label for="emanager">Name of Manager </label>
											<input id="emanager"  type="text" name="emanager" class="form-control" placeholder="Enter Manager name.." required>
										</div>
										<div class="form-group">
											<label for="email"><span id="entity5"></span> Email </label>
											<input id="signupEmail" onchange="emailValidation()" type="email" name="email" class="form-control" placeholder="Enter Email.." required>
										</div>
										<div class="form-group">
											<label for="password"><span id="entity6"></span> Password </label>
											<input id="signupPassword" onchange="passwordValidation()" type="password" name="password" class="form-control" placeholder="Enter a strong password.." required>
											<small id="passwordHelp" class="form-text text-muted">Minimum 10 characters long; include small, capital letters, & atleast 1 digit & 1 special character.</small>
										</div>
										<div class="form-group">
											<label for="confirmpassword">Confirm Password </label>
											<input id="confirmpassword" onchange="cpasswordValidation()" type="password" name="confirmpassword" class="form-control" placeholder="Re-enter password.." required>
											<small id="confirmpasswordHelp" class="form-text text-muted"></small>
										</div>
										<div class="form-group text-center">
                                            <button class="button button-submit btn-block register_button" type="button" id="signup">Sign Up</button>                                                                                </div>
										<div class="text-center">
											<h5 class="have">Have an account? <a class="red" href="/hospital/hospitallogin">Login</a></h5>
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>
